Vagrant.configure("2") do |config|
    # Configuraci칩n de la m치quina virtual
    config.vm.box = "ubuntu/bionic64"
    config.vm.hostname = "ubuntu-vm"
    config.vm.network "private_network", ip: "192.168.56.10"
  
    # Configuraci칩n de la red de host (Host-Only)
    config.vm.network "public_network", ip: "192.168.1.10", bridge: "en0: Wi-Fi (AirPort)" # Reemplaza "en0: Wi-Fi (AirPort)" con tu interfaz de red real
  
    # Configuraci칩n adicional
    config.vm.provider "virtualbox" do |vb|
      vb.memory = "1024"
      vb.cpus = 2
    end
  end
  